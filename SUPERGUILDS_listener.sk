    loop {_list::*}:
        set {_line::*} to loop-value split at "~"
        "%{_c}%" doesn't contain "%{_line::1}%: "
        set yaml value "Messages.%{_line::1}%" from "plugins/SUPERGUILDS/locale/%{_lang}%.yml" to "%{_line::2}%"
    save yaml "plugins/SUPERGUILDS/locale/%{_lang}%.yml"
    delete {SG::Messages::*}
    set {_messages::*} to SG_yamlList("Messages", "locale/%{_lang}%")
    loop {_messages::*}:
        set {SG::Messages::%loop-value%} to yaml value "Messages.%loop-value%" from "plugins/SUPERGUILDS/locale/%{_lang}%.yml"
    set {_now2} to now
    send "[&eSUPERGUILDS&r] %{_lang}% 加载完成! (%difference between {_now} and {_now2}%)" to console
on AsyncPlayerChatEvent:
    SG_checkFile("Players.Guild-Name", "playerdata/%event.getPlayer().getUniqueId()%") is not "*"
    SG_checkFile("Players.Chat-Enabled", "playerdata/%event.getPlayer().getUniqueId()%") is "true"
    event.setCancelled(true)
    set {_g} to SG_checkFile("Players.Guild-Name", "playerdata/%event.getPlayer().getUniqueId()%")
    set {_prefix} to "%{SG::Messages::Guild_Chat_Prefix}%"
    set {_rank} to SG_checkFile("Datas.Players.%event.getPlayer().getUniqueId()%.Rank", "guilds/%{_g}%")
    replace all "{PLAYER}" with "%event.getPlayer()%" in {_prefix}
    replace all "{RANK}" with "%{SG::Messages::Rank_%{_rank}%}%" in {_prefix}
    set {_list::*} to SG_yamlList("Datas.Players", "guilds/%{_g}%")
    loop {_list::*}:
        set {_p} to SG_checkFile("Datas.Players.%loop-value%.Username", "guilds/%{_g}%") parsed as offline player
        {_p} is online
        send "%{_prefix}% %event.getMessage()%" to {_p}
on join:
    file "plugins/SUPERGUILDS/playerdata/%uuid of player%.yml" doesn't exists:
        create file "plugins/SUPERGUILDS/playerdata/%uuid of player%.yml"
        SG_writeFile("Players.Race", "0", "playerdata/%uuid of player%")
        SG_writeFile("Players.Notifier", "0", "playerdata/%uuid of player%")
    SG_checkFile("Players.Inventory-Data", "playerdata/%uuid of player%") is "true":
        SG_playerInvAPI(player, "recover")
    SG_checkFile("Players.Username", "playerdata/%uuid of player%") is "*":
        SG_writeFile("Players.Username", "%player%", "playerdata/%uuid of player%")
    SG_writeFile("Players.Teleport-Status", "*", "playerdata/%uuid of player%")
    SG_writeFile("Players.Wipe-Command-Confirm", "*", "playerdata/%uuid of player%")
    SG_writeFile("Players.Remove-Command-Confirm", "*", "playerdata/%uuid of player%")
    SG_raceRegenEffect(player)

on death of player:
    SG_checkFile("Players.Guild-Name", "playerdata/%uuid of victim%") is not "*"
    set {_vGuild} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of victim%")
    attacker is a player
    # 是否有公会
    SG_checkFile("Players.Guild-Name", "playerdata/%uuid of attacker%") is not "*"
    set {_aGuild} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of attacker%")
    # 是否在对战内
    SG_checkFile("Datas.War-Status", "guilds/%{_aGuild}%") is "*"
    {_aGuild} is not {_vGuild}
    set {_price} to SG_checkFile("Guild-Management.Death-Penalty-Gold", "config") parsed as integer
    set {_vGold} to SG_checkFile("Datas.Resources.Gold.Has", "guilds/%{_vGuild}%") parsed as integer
    SG_valueRemoveAPI("Datas.Resources.Gold.Has", {_price}, "guilds/%{_vGuild}%")

    set {_msg} to {SG::Messages::Death_Broadcast}
    set {_vname} to SG_checkFile("Datas.Name", "guilds/%{_vGuild}%")
    set {_aname} to SG_checkFile("Datas.Name", "guilds/%{_aGuild}%")
    replace all "{VGUILD}" with "%{_vname}%" in {_msg}
    replace all "{AGUILD}" with "%{_aname}%" in {_msg}
    replace all "{ATTACKER}" with "%attacker%" in {_msg}
    replace all "{VICTIM}" with "%victim%" in {_msg}
    replace all "{GOLD}" with "%{_price}%" in {_msg}
    broadcast "%{SG::Messages::Prefix}% %{_msg}%"

    set {_msg} to {SG::Messages::Death_Gold_Loss}
    set {_rank} to SG_checkFile("Datas.Players.%uuid of victim%.Rank", "guilds/%{_vGuild}%")
    set {_name} to SG_checkFile("Guild-Management.Ranks.%{_rank}%-Name", "config")
    replace all "{PLAYER}" with "%victim%" in {_msg}
    replace all "{GOLD}" with "%{_price}%" in {_msg}
    replace all "{RANK}" with "%{_name}%" in {_msg}
    SG_guildBroadcast("%{_vGuild}%", "%{_msg}%")   
    # 执行额外指令
    SG_checkFile("General.Executable-Commands.On-Death", "config") is not "*"
    set {_executable} to SG_checkFile("General.Executable-Commands.On-Death", "config")
    replace all "{VICTIM}" with "%player%" in {_executable}
    replace all "{ATTACKER}" with "%attacker%" in {_executable}
    execute console command "%{_executable}%"
# 伤害相关
on damage of player:
    if attacker is a player:
        victim is not attacker
        set {_vg} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of victim%")
        set {_ag} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of attacker%")
        {_vg} is not "*"
        {_ag} is not "*"
        if "%{_ag}%" is "%{_vg}%":
            SG_checkFile("Players.PVP", uuid of attacker) is "false" or "*"
            send "%{SG::Messages::Prefix}% %{SG::Messages::Attack_GuildMembers}%" to attacker
            cancel event
        else:
            set {_allies::*} to SG_yamlList("Datas.Allies", "guilds/%{_ag}%")
            if {_allies::*} contains "%{_vg}%":
                send "%{SG::Messages::Prefix}% %{SG::Messages::Cannot_War_Allies}%" to attacker 
                cancel event
            else:
                set {_aLevel} to SG_checkFile("Datas.Buildings.Alchemy.Effect-3-Level", "guilds/%{_ag}%") parsed as integer
                set {_dLevel} to SG_checkFile("Datas.Buildings.Alchemy.Effect-4-Level", "guilds/%{_vg}%") parsed as integer
                {_aLevel} is set
                {_dLevel} is set
                cancel event
                set {_damage} to damage
                add 5 * {_aLevel} to {_damage}
                remove 5 * {_dLevel} from {_damage}
                damage victim by {_damage}
    else:
        SG_checkFile("Regions", "chunks/%chunk at location of victim%") is not "*"
        SG_checkFile("General.Claim-Protection.Damage-Protection", "config") is "true" or "*"
        cancel event
# 领地相关
on PlayerInteractEvent with priority highest:
    event.getPlayer() doesn't have permission "guilds.admin" or "guilds.bypass"
    "%event.getAction()%" is "RIGHT_CLICK_BLOCK" or "LEFT_CLICK_BLOCK":
        SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is "*":
            set {_eWorld::*} to SG_checkFile("General.Interact-Only-Claimed-Areas-Enabled-Worlds", "config") split at ","
            loop {_eWorld::*}:
                "%loop-value%" is "%world of event.getPlayer()%"
                event.setCancelled(true)
                exit loop
            send "%{SG::Messages::Prefix}% %{SG::Messages::Break_Claimed_Areas_Only}%" to event.getPlayer()
        SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is not "*":
            SG_checkFile("General.Claim-Protection.Left-Click", "config") is "true" or "*"
            SG_checkFile("General.Claim-Protection.Right-Click", "config") is "true" or "*"
            set {_pGuild} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of event.getPlayer()%")
            SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is not "%{_pGuild}%":
                set {_msg} to {SG::Messages::Owned_By}
                set {_g} to SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%")
                set {_name} to SG_checkFile("Datas.Name", "guilds/%{_g}%")
                replace all "{GUILD}" with "%{_name}%" in {_msg}
                send "%{SG::Messages::Prefix}% %{_msg}%" to event.getPlayer()
                event.setCancelled(true)
            SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is "%{_pGuild}%":
                SG_rankCheck("%event.getPlayer().getUniqueId()%", "Interact") is false
                send "%{SG::Messages::Prefix}% %{SG::Messages::No_Rank}%" to event.getPlayer()
                event.setCancelled(true)
    "%event.getAction()%" = "PHYSICAL":
        SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is "*":
            set {_eWorld::*} to SG_checkFile("General.Interact-Only-Claimed-Areas-Enabled-Worlds", "config") split at ","
            loop {_eWorld::*}:
                "%loop-value%" is "%world of event.getPlayer()%"
                event.setCancelled(true)
            send "%{SG::Messages::Prefix}% %{SG::Messages::Break_Claimed_Areas_Only}%" to event.getPlayer()
        SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is not "*":
            SG_checkFile("General.Claim-Protection.Left-Click", "config") is "true" or "*"
            SG_checkFile("General.Claim-Protection.Right-Click", "config") is "true" or "*"
            set {_pGuild} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of event.getPlayer()%")
            SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is not "%{_pGuild}%":
                set {_msg} to {SG::Messages::Owned_By}
                set {_g} to SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%")
                set {_name} to SG_checkFile("Datas.Name", "guilds/%{_g}%")
                replace all "{GUILD}" with "%{_name}%" in {_msg}
                send "%{SG::Messages::Prefix}% %{_msg}%" to event.getPlayer()
                event.setCancelled(true)
            SG_checkFile("Regions", "chunks/%event.getClickedBlock().getLocation().getChunk()%") is "%{_pGuild}%":
                SG_rankCheck("%event.getPlayer().getUniqueId()%", "Interact") is false
                send "%{SG::Messages::Prefix}% %{SG::Messages::No_Rank}%" to event.getPlayer()
                event.setCancelled(true)
on BlockBreakEvent:
    event.getPlayer() doesn't have permission "guilds.admin" or "guilds.bypass"
    event.setCancelled(true)
    SG_checkFile("Regions", "chunks/%event.getBlock().getLocation().getChunk()%") is "*":
        set {_eWorld::*} to SG_checkFile("General.Interact-Only-Claimed-Areas-Enabled-Worlds", "config") split at ","
        loop {_eWorld::*}:
            "%loop-value%" is not "%world of event.getPlayer()%"
            event.setCancelled(false)
            stop
        send "%{SG::Messages::Prefix}% %{SG::Messages::Break_Claimed_Areas_Only}%" to event.getPlayer()
    SG_checkFile("Regions", "chunks/%event.getBlock().getLocation().getChunk()%") is not "*":
        SG_checkFile("General.Claim-Protection.Left-Click", "config") is "true" or "*"
        set {_pGuild} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of event.getPlayer()%")
        SG_checkFile("Regions", "chunks/%event.getBlock().getLocation().getChunk()%") is not "%{_pGuild}%":
            set {_msg} to {SG::Messages::Owned_By}
            set {_g} to SG_checkFile("Regions", "chunks/%event.getBlock().getLocation().getChunk()%")
            set {_name} to SG_checkFile("Datas.Name", "guilds/%{_g}%")
            replace all "{GUILD}" with "%{_name}%" in {_msg}
            send "%{SG::Messages::Prefix}% %{_msg}%" to event.getPlayer()
        SG_checkFile("Regions", "chunks/%event.getBlock().getLocation().getChunk()%") is "%{_pGuild}%":
            SG_rankCheck("%event.getPlayer().getUniqueId()%", "Break") is false:
                send "%{SG::Messages::Prefix}% %{SG::Messages::No_Rank}%" to event.getPlayer()
            SG_rankCheck("%event.getPlayer().getUniqueId()%", "Break") is true:
                event.setCancelled(false)
