on damage of armor stand:
    attacker doesn't have permission "guilds.admin" or "guilds.bypass"
    attacker is a player
    set {_pGuild} to SG_checkFile("Players.Guild-Name", "attackerdata/%uuid of attacker%")
    set {_chunk} to SG_checkFile("Regions", "chunks/%chunk at location of victim%")
    {_chunk} is "*":
        set {_eWorld} to SG_checkFile("General.Break-Only-Claimed-Areas-Enabled-Worlds", "config")
        "%{_eWorld}%" contains "%world of attacker%":
            cancel event
        send "%{SG::Messages::Prefix}% %{SG::Messages::Break_Claimed_Areas_Only}%" to attacker
    {_chunk} is not "*":
        SG_checkFile("General.Claim-Protection.Left-Click", "config") is "false":
            stop
        if {_chunk} is not "%{_pGuild}%":
            cancel event
            set {_msg} to {SG::Messages::Owned_By}
            set {_name} to SG_checkFile("Datas.Name", "guilds/%{_chunk}%")
            replace all "{GUILD}" with "%{_name}%" in {_msg}
            send "%{SG::Messages::Prefix}% %{_msg}%" to attacker
        else:
            SG_rankCheck(uuid of attacker, "Break") is false
            cancel event
            send "%{SG::Messages::Prefix}% %{SG::Messages::No_Rank}%" to attacker
on spawn of armor stand:
    player doesn't have permission "guilds.admin" or "guilds.bypass"
    set {_pGuild} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%")
    set {_chunk} to SG_checkFile("Regions", "chunks/%chunk at location of event-entity%")
    {_chunk} is "*":
        set {_eWorld} to SG_checkFile("General.Break-Only-Claimed-Areas-Enabled-Worlds", "config")
        "%{_eWorld}%" contains "%world of player%":
            cancel event
        send "%{SG::Messages::Prefix}% %{SG::Messages::Place_Claimed_Areas_Only}%" to player
    {_chunk} is not "*":
        SG_checkFile("General.Claim-Protection.Right-Click", "config") is "false":
            stop
        if {_chunk} is not "%{_pGuild}%":
            cancel event
            set {_msg} to {SG::Messages::Owned_By}
            set {_name} to SG_checkFile("Datas.Name", "guilds/%{_chunk}%")
            replace all "{GUILD}" with "%{_name}%" in {_msg}
            send "%{SG::Messages::Prefix}% %{_msg}%" to player
        else:
            SG_rankCheck(uuid of player, "Place") is false
            cancel event
            send "%{SG::Messages::Prefix}% %{SG::Messages::No_Rank}%" to player
# 变量相关
on placeholder request with prefix "superguilds":
    identifier is "guild":
        SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%") is "*":
            set the result to "無"
        SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%") is not "*":
            set {_g1} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%")
            set {_result} to SG_checkFile("Datas.Name", "guilds/%{_g1}%")
            set the result to {_result}
    identifier is "race":
        SG_checkFile("Players.Race", "playerdata/%uuid of player%") is "*":
            set the result to "無"
        SG_checkFile("Players.Race", "playerdata/%uuid of player%") is not "*":
            set {_race} to SG_checkFile("Players.Race", "playerdata/%uuid of player%")
            set {_result} to SG_checkFile("Races.%{_race}%.Name", "races")
            set the result to {_result}
    identifier is "rank":
        SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%") is "*":
            set the result to "無"
        SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%") is not "*":
            set {_g2} to SG_checkFile("Players.Guild-Name", "playerdata/%uuid of player%")
            set {_rank} to SG_checkFile("Datas.Players.%uuid of player%.Rank", "guilds/%{_g2}%")
            set {_result} to SG_checkFile("Guild-Management.Ranks.%{_rank}%-Name", "config")
            set the result to {_result}